{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "version": "1.0.5",
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "applications": {
    "gecko": {
      "id": "mousecommander-native@addons.mozilla.org",
      "strict_min_version": "68.0a1"
    }
  },
  "permissions": [
    "<all_urls>",
    "bookmarks",
    "browserSettings",
    "find",
    "nativeMessaging",
    "sessions",
    "storage",
    "tabs"
  ],
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": true,
    "browser_style": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "page": "src/background.html"
  }
}