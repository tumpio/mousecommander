{
  "events": [
    {
      "type": "scroll_up",
      "buttonsDown": ["secondary"],
      "command": "switchToNextTab"
    },
    {
      "type": "scroll_down",
      "buttonsDown": ["secondary"],
      "command": "switchToPreviousTab"
    },
    {
      "type": "scroll_up",
      "buttonsDown": ["primary"],
      "command": "increaseZoom"
    },
    {
      "type": "scroll_down",
      "buttonsDown": ["primary"],
      "command": "decreaseZoom"
    },
    {
      "type": "mouse_down",
      "button": "middle",
      "buttonsDown": ["primary"],
      "command": "resetZoom"
    },
    {
      "type": "mouse_up",
      "button": "middle",
      "buttonsDown": ["secondary"],
      "command": "closeCurrentTab"
    },
    {
      "type": "long_press",
      "button": "middle",
      "buttonsDown": ["secondary"],
      "command": "restoreClosedTab"
    }
  ],
  "sequences": [
    {
      "sequence": "c12,c02,c12,c02",
      "command": "highlightSelectedText"
    },
    {
      "sequence": "c12,c02,c12,c22",
      "command": "toggleBookmark"
    }
  ]
}
